    mainFrame.Visible = false
end)

miniGui.MouseButton1Click:Connect(function()
    mainFrame.Visible = not mainFrame.Visible
end)

-- Auto Farm Linh Khi
spawn(function()
    while true do
        wait(1)
        TuViSystem:AddLinhKhi(1)
        if TuViDisplay then
            TuViDisplay.Text = string.format("Tu Vi: %s\nLinh Khi: %d/100", 
                TuViSystem.Levels[TuViSystem.CurrentLevel], TuViSystem.LinhKhi)
        end
    end
end)

