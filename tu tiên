-- Khu Main
local function ShowMainTab()
    ClearContent()
    currentTab = "Main"
    
    local mainContent = Instance.new("Frame")
    mainContent.Size = UDim2.new(1, 0, 1, 0)
    mainContent.BackgroundTransparency = 1
    mainContent.Parent = contentFrame
    
    local yPosition = 0
    local buttonHeight = 35
    local gap = 5
    
    -- GodMode
    CreateButton("GodMode: " .. (godModeEnabled and "ON" or "OFF"), 
        UDim2.new(0, 0, 0, yPosition), function(button)
        godModeEnabled = not godModeEnabled
        if godModeEnabled then
            humanoid.MaxHealth = math.huge
            humanoid.Health = math.huge
            button.Text = "GodMode: ON"
        else
            humanoid.MaxHealth = 100
            humanoid.Health = 100
            button.Text = "GodMode: OFF"
        end
    end, mainContent)
    yPosition = yPosition + buttonHeight + gap
    
    -- Invisible
    CreateButton("Invisible: " .. (invisibleEnabled and "ON" or "OFF"), 
        UDim2.new(0, 0, 0, yPosition), function(button)
        invisibleEnabled = not invisibleEnabled
        if invisibleEnabled then
            for _, part in pairs(character:GetChildren()) do
                if part:IsA("BasePart") then
                    part.Transparency = 1
                end
            end
            button.Text = "Invisible: ON"
        else
            for _, part in pairs(character:GetChildren()) do
                if part:IsA("BasePart") then
                    part.Transparency = 0
                end
            end
            button.Text = "Invisible: OFF"
        end
    end, mainContent)
    yPosition = yPosition + buttonHeight + gap
    
    -- ESP
    CreateButton("ESP: " .. (espEnabled and "ON" or "OFF"), 
        UDim2.new(0, 0, 0, yPosition), function(button)
        espEnabled = not espEnabled
        button.Text = "ESP: " .. (espEnabled and "ON" or "OFF")
        if espEnabled then
            for _, otherPlayer in pairs(Players:GetPlayers()) do
                if otherPlayer ~= player then
                    CreateESP(otherPlayer)
                end
            end
        else
            for _, box in pairs(espBoxes) do
                if box then box:Destroy() end
            end
            espBoxes = {}
        end
    end, mainContent)
    yPosition = yPosition + buttonHeight + gap
    
    -- Fake Lag
    CreateButton("Fake Lag: " .. (fakeLagEnabled and "ON" or "OFF"), 
        UDim2.new(0, 0, 0, yPosition), function(button)
        fakeLagEnabled = not fakeLagEnabled
        button.Text = "Fake Lag: " .. (fakeLagEnabled and "ON" or "OFF")
        if fakeLagEnabled then
            game:GetService("NetworkClient"):SetOutgoingKBPSLimit(10)
        else
            game:GetService("NetworkClient"):SetOutgoingKBPSLimit(1000)
        end
    end, mainContent)
    yPosition = yPosition + buttonHeight + gap
    
    -- Fix Lag
    CreateButton("Fix Lag", UDim2.new(0, 0, 0, yPosition), function()
        for _, obj in pairs(workspace:GetDescendants()) do
            if obj:IsA("ParticleEmitter") or obj:IsA("Fire") or obj:IsA("Smoke") then
                obj:Destroy()
            end
        end
        settings().Rendering.QualityLevel = 1
    end, mainContent)
    yPosition = yPosition + buttonHeight + gap
    
    -- Speed Control
    CreateButton("Speed Control", UDim2.new(0, 0, 0, yPosition), function()
        humanoid.WalkSpeed = 50
    end, mainContent)
end

