-- T·∫°o tab buttons
for i, tab in ipairs(tabs) do
    local tabName, tabColor = tab[1], tab[2]
    local tabButton = Instance.new("TextButton")
    tabButton.Size = UDim2.new(0.3, -5, 1, 0)
    tabButton.Position = UDim2.new((i-1)*0.33, 0, 0, 0)
    tabButton.BackgroundColor3 = tabColor
    tabButton.TextColor3 = Color3.new(1, 1, 1)
    tabButton.Text = tabName
    tabButton.Font = Enum.Font.GothamBold
    tabButton.TextSize = 12
    tabButton.Parent = tabButtonsFrame
    
    tabButton.MouseButton1Click:Connect(function()
        if tabName == "Th√¥ng Tin" then
            ShowThongTinTab()
        elseif tabName == "Main" then
            ShowMainTab()
        elseif tabName == "C√¥ng Ph√°p" then
            ShowCongPhapTab()
        end
    end)
end

-- ESP Function
function CreateESP(targetPlayer)
    local character = targetPlayer.Character
    if not character then return end
    
    local highlight = Instance.new("Highlight")
    highlight.FillColor = Color3.new(1, 0, 0)
    highlight.OutlineColor = Color3.new(1, 1, 1)
    highlight.FillTransparency = 0.5
    highlight.Parent = character
    highlight.Adornee = character
    
    espBoxes[targetPlayer] = highlight
end

-- Thi√™n Ki·∫øm Function
function SpawnThienKiem()
    local sword = Instance.new("Part")
    sword.Name = "ThienKiem"
    sword.Size = Vector3.new(2, 8, 1)
    sword.BrickColor = BrickColor.new("Bright yellow")
    sword.Material = Enum.Material.Neon
    sword.Position = rootPart.Position + Vector3.new(0, 15, 0)
    sword.Anchored = true
    sword.CanCollide = false
    sword.Parent = workspace
    
    local fire = Instance.new("Fire")
    fire.Color = Color3.new(1, 1, 0)
    fire.SecondaryColor = Color3.new(1, 0.5, 0)
    fire.Size = 8
    fire.Parent = sword
    
    local light = Instance.new("PointLight")
    light.Color = Color3.new(1, 1, 0)
    light.Brightness = 10
    light.Range = 25
    light.Parent = sword
    
    -- Ai c≈©ng c√≥ th·ªÉ th·∫•y
    sword.Changed:Connect(function()
        -- ƒê·∫£m b·∫£o ki·∫øm lu√¥n hi·ªÉn th·ªã
    end)
    
    game:GetService("Debris"):AddItem(sword, 30)
end

-- Ph√¢n Th√¢n Function
function CreatePhanThan()
    local clone = character:Clone()
    clone.Name = "PhanThan_" .. player.Name
    clone.Parent = workspace
    
    -- ƒê·∫∑t v·ªã tr√≠ g·∫ßn character ch√≠nh
    local humanoidRootPart = clone:FindFirstChild("HumanoidRootPart")
    if humanoidRootPart then
        humanoidRootPart.Position = rootPart.Position + Vector3.new(5, 0, 0)
    end
    
    -- Animation ph√¢n th√¢n
    for _, part in pairs(clone:GetChildren()) do
        if part:IsA("BasePart") then
            part.BrickColor = BrickColor.new("Bright blue")
            part.Transparency = 0.3
        end
    end
    
    -- T·ª± h·ªßy sau 30s
    delay(30, function()
        if clone then
            clone:Destroy()
        end
    end)
end

