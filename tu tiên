        "H·ªón Nguy√™n Ho√†ng", "H·ªón Nguy√™n ƒê·∫ø", "H·ªìng M√¥ng Gi·∫£", "H·ªìng M√¥ng Linh Gi·∫£", 
        "H·ªìng M√¥ng Huy·ªÅn Gi·∫£", "H·ªìng M√¥ng Ho√†ng Gi·∫£", "H·ªìng M√¥ng Th√°nh Gi·∫£", "H·ªìng M√¥ng Thi√™n Gi·∫£"
    },
    CurrentLevel = 1,
    LinhKhi = 0,
    DataKey = "TuViData_" .. player.UserId
}

function TuViSystem:SaveData()
    local data = {Level = self.CurrentLevel, LinhKhi = self.LinhKhi}
    pcall(function()
        game:GetService("DataStoreService"):GetDataStore("TuVi"):SetAsync(self.DataKey, data)
    end)
end

function TuViSystem:LoadData()
    pcall(function()
        local data = game:GetService("DataStoreService"):GetDataStore("TuVi"):GetAsync(self.DataKey)
        if data then
            self.CurrentLevel = data.Level or 1
            self.LinhKhi = data.LinhKhi or 0
        end
    end)
end

function TuViSystem:AddLinhKhi(amount)
    self.LinhKhi = self.LinhKhi + amount
    if self.LinhKhi >= 100 then
        self.LinhKhi = 0
        self.CurrentLevel = math.min(self.CurrentLevel + 1, #self.Levels)
        self:LevelUpEffect()
    end
    self:UpdateDisplay()
    self:SaveData()
end

function TuViSystem:LevelUpEffect()
    local effect = Instance.new("Part")
    effect.Size = Vector3.new(8, 8, 8)
    effect.Position = rootPart.Position
    effect.Anchored = true
    effect.CanCollide = false
    effect.Material = Enum.Material.Neon
    effect.BrickColor = BrickColor.new("Bright yellow")
    effect.Parent = workspace
    
    local sound = Instance.new("Sound")
    sound.SoundId = "rbxassetid://138186576"
    sound.Volume = 1
    sound.Parent = effect
    sound:Play()
    
    game:GetService("Debris"):AddItem(effect, 3)
end

-- C√¥ng Ph√°p System
local CongPhapSystem = {
    ["C·ª≠u Chuy·ªÉn Kim ƒêan C√¥ng"] = {
        Damage = 500,
        Color = "Bright yellow",
        Effect = "Explosion"
    },
    ["Th√°i C·ª±c √Çm D∆∞∆°ng ƒê·∫°o"] = {
        Damage = 800, 
        Color = "Black",
        Effect = "Ring"
    },
    ["H·ªón ƒê·ªôn Khai Thi√™n Tr·∫£o"] = {
        Damage = 1200,
        Color = "Really red", 
        Effect = "Beam"
    }
}

function CreateCongPhapTools()
    for name, data in pairs(CongPhapSystem) do
        local tool = Instance.new("Tool")
        tool.Name = name
        tool.ToolTip = "C√¥ng Ph√°p: " .. name
        tool.CanBeDropped = false
        
        local handle = Instance.new("Part")
        handle.Name = "Handle"
        handle.Size = Vector3.new(1, 1, 1)
        handle.BrickColor = BrickColor.new(data.Color)
        handle.Material = Enum.Material.Neon
        handle.Parent = tool
        
        tool.Activated:Connect(function()
            CastCongPhap(name)
        end)
        
        tool.Parent = player.Backpack
    end
end

function CastCongPhap(congPhapName)
    local congPhap = CongPhapSystem[congPhapName]
    if congPhap then
        -- Hi·ªáu ·ª©ng c√¥ng ph√°p
        local mouse = player:GetMouse()
        local targetPos = mouse.Hit.Position
        
        if congPhap.Effect == "Explosion" then
            local explosion = Instance.new("Explosion")
            explosion.Position = targetPos
            explosion.BlastPressure = congPhap.Damage
            explosion.BlastRadius = 15
            explosion.Visible = true
            explosion.Parent = workspace
        elseif congPhap.Effect == "Ring" then
            local ring = Instance.new("Part")
            ring.Size = Vector3.new(10, 1, 10)
            ring.Position = targetPos
            ring.Anchored = true
            ring.CanCollide = false
            ring.BrickColor = BrickColor.new(congPhap.Color)
            ring.Material = Enum.Material.Neon
            ring.Parent = workspace
            game:GetService("Debris"):AddItem(ring, 3)
        end
        
        TuViSystem:AddLinhKhi(5) -- Th√™m linh kh√≠ khi s·ª≠ d·ª•ng c√¥ng ph√°p
    end
end

